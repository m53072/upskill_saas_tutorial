<div class="containter">
  <h3 class="text-center">Contact Form</h1>

  <%= form_for "/contact" do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    
    <%= f.label :email %>
    <%= f.text_field :email %>
    
    <%= f.label :comments %>
    <%= f.text_field :comments %>
    
    <%= f.submit 'Submit' %>
  <% end %>
</div>